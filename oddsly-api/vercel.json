{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/create-payment-intent",
        "dest": "/api/create-payment-intent.ts"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.ts"
      }
    ]
}